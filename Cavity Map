#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main(){
    int n;
    cin >> n;
    vector<string> grid(n);
    for(int grid_i = 0;grid_i < n;grid_i++){
       cin >> grid[grid_i];
    }
    
    for(int i=1;i<n;i++){
        if(grid[i]>grid[i-1]&&grid[i]>grid[i+1])
            grid[i]='x';
        else
            grid[i]=grid[i];
            }
    
    for(int u=0;u<n;u++){
        cout<<grid[u];
    }
    
    
    
    
    
    
    return 0;
}
